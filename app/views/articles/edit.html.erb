
<% form_for(@article) do |f| %>
<%= f.error_messages %>

<p>
  <%= f.label :title %><br />
  <%= f.text_field :title %>
</p>
<p>
  <%= f.label :url %><br />
  <%= f.text_field :url %>
</p>
<p>
  <%= f.label :author %><br />
  <%= f.text_field :author %>
</p>


<p>
  <%= f.submit @article.new_record? ? 'Create' :'Update' %>
  <%= button_to "Delete", @article, :confirm => 'Are you sure?', :method => :delete %>
</p>
  <% end %>

<% unless @article.new_record? %>
<%#= form_for(@article.article_tags.build) do |f| %>



<%# end %>
<% end %>